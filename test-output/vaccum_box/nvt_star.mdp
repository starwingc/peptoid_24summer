title                   = 19AE1-4-A_protonated_GMX_vacuum

; Run parameters
integrator              = md        ; leap-frog integrator
nsteps                  = 50000     ; 2 * 50000 = 100 ps
dt                      = 0.0005     ; 2 fs

; Output control
nstxout                 = 500       ; save coordinates every 1.0 ps
nstvout                 = 500       ; save velocities every 1.0 ps
nstenergy               = 500       ; save energies every 1.0 ps
nstlog                  = 500       ; update log file every 1.0 ps

; Bond parameters
continuation            = no        ; first dynamics run
; constraint_algorithm    = lincs     ; holonomic constraints 
; constraints             = h-bonds   ; bonds involving H are constrained
; lincs_iter              = 1         ; accuracy of LINCS
; lincs_order             = 4         ; also related to accuracy

; Nonbonded settings 
cutoff-scheme           = 2e-03     ; Buffered neighbor searching
ns_type                 = grid      ; search neighboring grid cells
nstlist                 = 10        ; 20 fs, largely irrelevant with Verlet
rvdw                    = 1.2       ; short-range van der Waals cutoff (in nm)
DispCorr                = EnerPres  ; account for cut-off vdW scheme

; Electrostatics
;coulombtype             = PME       ; Particle Mesh Ewald for long-range electrostatics
coulombtype           = Cut-off  ; Plain cut-off with pair list radius rlist and Coulomb cut-off rcoulomb, where rlist >= rcoulomb
rcoulomb                = 1.2        
pme_order               = 4         ; cubic interpolation
fourierspacing          = 0.16      ; grid spacing for FFT

; Temperature coupling is on
tcoupl                  = V-rescale             ; modified Berendsen thermostat
tc-grps                 = System  		; only in vaccum 
tau_t                   = 0.05                   ; time constant, in ps
ref_t                   = 5                   ; reference temperature, one for each group, in K

; Pressure coupling is off
pcoupl                  = no        ; no pressure coupling in NVT
; Periodic boundary conditions
pbc                     = xyz       ; 3-D PBC
; Velocity generation
gen_vel                 = no       ; assign velocities from Maxwell distribution
gen_temp                = 5        ; temperature for Maxwell distribution
gen_seed                = -1       ; generate a random seed
